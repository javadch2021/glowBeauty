{
  "name": "learningremix",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix vite:build",
    "build:server": "tsc --project tsconfig.server.json",
    "build:all": "npm run build && npm run build:server",
    "dev": "vite-node ./server.ts",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "start": "cross-env NODE_ENV=production vite-node ./server.ts",
    "start:compiled": "cross-env NODE_ENV=production node ./build/server.js",
    "typecheck": "tsc",
    "typecheck:server": "tsc --project tsconfig.server.json --noEmit",
    "seed": "tsx scripts/seed.ts",
    "seed:clear": "tsx scripts/seed.ts clear",
    "seed:reset": "tsx scripts/seed.ts reset",
    "seed:status": "tsx scripts/seed.ts status"
  },
  "dependencies": {
    "@remix-run/express": "^2.16.8",
    "@remix-run/node": "^2.16.8",
    "@remix-run/react": "^2.16.8",
    "@types/mongodb": "^4.0.7",
    "bcryptjs": "^3.0.2",
    "compression": "^1.7.4",
    "cookie": "^1.0.2",
    "cross-env": "^7.0.3",
    "express": "^4.19.2",
    "express-rate-limit": "^8.0.1",
    "helmet": "^8.1.0",
    "isbot": "^4.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^6.17.0",
    "morgan": "^1.10.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@remix-run/dev": "^2.16.8",
    "@types/bcryptjs": "^3.0.0",
    "@types/compression": "^1.7.5",
    "@types/cookie": "^1.0.0",
    "@types/express": "^4.17.20",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.9",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.38.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.4",
    "tsx": "^4.20.3",
    "typescript": "^5.1.6",
    "vite": "^6.0.0",
    "vite-tsconfig-paths": "^4.2.1"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
